#include<bits/stdc++.h>//rimi
using namespace std;
#define max 100005
int ar[max];
vector<int >tree(max*4);
int k=1;

int main()
{

    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

	unsigned long  long int x,y,z,m,a,b,c,t,i,j,n,k,h,e,w,p,q,l,o,d;
	string s1,s,s2,str;
 //tree.resize(4*n);
    map<char, int>ma;
   while(cin>>a>>b>>c){

    k =1;
    a = a%c;
   while(b>0)
   {
       if(b&1)
       {
           k = (k*a)%c;
       }
       a = (a*a)%c;
       b>>=1;

   }
   cout<<k<<endl;

   }

    return 0;
}
